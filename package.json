{
  "name": "hexo-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "hexo generate",
    "clean": "hexo clean",
    "deploy": "hexo deploy",
    "server": "hexo server",
    "drop": "rm source/_posts/* -rf",
    "convert": "node --experimental-transform-types ./convert.ts",
    "fixCategories": "node --experimental-transform-types ./fix-categories.ts",
    "restoreMd": "git checkout HEAD ./source/_posts/*/*/*.md && git checkout HEAD ./source/_posts/*/*.md && git checkout HEAD ./source/_posts/*.md"
  },
  "hexo": {
    "version": "7.3.0"
  },
  "dependencies": {
    "hexo": "^7.3.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^7.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0"
  },
  "devDependencies": {
    "hexo-renderer-pug": "^3.0.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-theme-butterfly": "^5.4.2",
    "p-map": "^7.0.3"
  }
}